<!DOCTYPE html>
<html>
<head>
  <% include ../includes/header.ejs %>
</head>

<body>
  <% include ../includes/nav.ejs %>
<div class="jumbotron text-center">
  <div class="container">
    <h1><%= title %></h1>
    <p>  
      <ul>
      <% for (i = 0; i < itemsData.length; i++) { %>
      <li><h3><%= itemsData[i].name %></h3>
        <img src="<%= itemsData[i].imageUrl %>">
        <h6><%= itemsData[i].price %></h6>
        <p><%= itemsData[i].description %></p>
        <p>
          Tags: <% for (j = 0; j < itemsData[i].tags.length; j++) { %>
            <%= itemsData[i].tags[j] %>
            <% } %>
        </p>
      </li>
      <% } %>
      </ul>
    </p>
  </div>
</div>
<div class="container">
  
</div>


</body>
</html>
